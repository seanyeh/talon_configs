tag: terminal
-
mux: "tmux"
mux new:
  key(ctrl-a)
  "c"
mux close:
  key(ctrl-a)
  "x"
mux horizontal:
  key(ctrl-a)
  "s"
mux vertical:
  key(ctrl-a)
  "v"
mux toggle:
  key(ctrl-a)
  "z"
mux up:
  key(ctrl-a)
  "k"
mux down:
  key(ctrl-a)
  "j"
mux left:
  key(ctrl-a)
  "h"
mux right:
  key(ctrl-a)
  "l"
mux scroll:
  key(ctrl-a)
  "["
  key(pageup)

